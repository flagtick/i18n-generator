{
    "name": "flagtickgroup/i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": ["laravel","vue-i18n"],
    "homepage": "https://github.com/flagtick/i18n-generator",
    "authors": [
        {
            "name": "Flagtick Group",
            "email": "admin@flagtickgroup.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FlagtickGroup\\InternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FlagtickGroup\\InternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
